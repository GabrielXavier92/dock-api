{
  "name": "dock-api",
  "version": "1.0.0",
  "description": "Dock api for job test ",
  "main": "src/server.ts",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "sucrase ./src -d ./build --transforms typescript,imports",
    "test": "jest --watch --no-cache",
    "test:ci": "jest --coverage --noStackTrace --silent",
    "database-migrations": "npx knex migrate:up",
    "database-seed": "npx knex seed:run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GabrielXavier92/dock-api.git"
  },
  "author": "Gabriel Xavier",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GabrielXavier92/dock-api/issues"
  },
  "homepage": "https://github.com/GabrielXavier92/dock-api#readme",
  "dependencies": {
    "@types/node": "^14.0.23",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "4.16.4",
    "husky": "^4.2.5",
    "knex": "^0.21.2",
    "pg": "^8.3.0",
    "swagger-ui-express": "^4.1.4",
    "typescript": "^3.9.6"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.7",
    "@types/jest": "^26.0.4",
    "@types/swagger-ui-express": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "eslint": "^7.1.0",
    "eslint-config-airbnb-typescript": "^7.2.1",
    "eslint-plugin-import": "^2.20.1",
    "jest": "^26.1.0",
    "sucrase": "^3.15.0",
    "ts-jest": "^26.1.2",
    "ts-node-dev": "^1.0.0-pre.51"
  }
}
